{
  "extends": ["@vue/tsconfig/tsconfig.dom.json", "../tsconfig.json"],
  "include": ["./e2e/**/*", "./support/**/*", "**/*.ts"],
  "exclude": ["./support/component.*"],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "node"],
    "isolatedModules": true,
    "noEmit": true
  }
}
