{
  "name": "pix_link",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "build:portable": "npm run build && npm run zip",
    "zip": "cd dist && zip -r ../dist.zip .",
    "prepare": "cypress install",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "test:all": "npm run test:unit && npm run test:e2e:ci",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:dev": "cypress open --component",
    "test:unit:component": "cypress run --component",
    "test:e2e": "cypress open --e2e",
    "test:e2e:ci": "cypress run --e2e",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:preview": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@sentry/vue": "^10.32.1",
    "@tailwindcss/vite": "^4.1.17",
    "@vercel/analytics": "^1.6.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "crypto-js": "^4.2.0",
    "form-data": "^4.0.5",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qrcode": "^1.5.4",
    "qs": "^6.14.1",
    "tmp": "^0.2.5",
    "vue": "^3.5.22",
    "vue-icons-plus": "^0.1.8",
    "vue-toast-notification": "^3.1.3",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "cypress": "^15.8.1",
    "cypress-real-events": "^1.15.0",
    "eslint": "^9.37.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.1"
  }
}
